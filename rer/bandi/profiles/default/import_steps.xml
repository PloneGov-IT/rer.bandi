<?xml version="1.0"?>
<import-steps>

  <import-step
    id="rer.bandi-versioning"
    version="20110401-01"
    handler="rer.bandi.setuphandlers.setVersionedTypes"
    title="rer.bandi: Version">
      Setup handler to put under version control the specified portal types
    <dependency step="typeinfo"/>
  </import-step>

  <import-step
    id="rer.bandi-indexes"
    version="20110411-01"
    handler="rer.bandi.setuphandlers.add_catalog_indexes"
    title="rer.bandi catalog indexes">
      Setup handler to add indexes to catalog
  </import-step>
  
  <import-step
    id="rer.bandi-dest"
    version="20110411-01"
    handler="rer.bandi.setuphandlers.addPropertySheet"
    title="rer.bandi bandi propertysheet">
      Setup handler to add property sheet
  </import-step>
  
</import-steps>
